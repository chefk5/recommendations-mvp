{
  "name": "home-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.json --max-warnings 0",
    "lint-and-fix": "eslint . --ext .ts,.tsx,.js,.jsx,.json --fix",
    "prettier-format": "prettier --config .prettierrc '**/*.{json,js,jsx,ts,tsx,css,scss,md}' --write",
    "pre-commit": "yarn prettier-format && yarn lint",
    "prepare": "husky install",
    "test": "jest --watch"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.2",
    "@shopify/flash-list": "1.7.6",
    "@tanstack/react-query": "^5.82.0",
    "axios": "^1.10.0",
    "contentful": "^11.7.6",
    "expo": "^53.0.19",
    "expo-dev-client": "~5.2.4",
    "expo-image": "~2.3.2",
    "expo-linking": "~7.1.7",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-content-loader": "^7.0.2",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "~3.17.4",
    "react-native-reanimated-carousel": "^4.0.2",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.72.0",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "jest": "^30.0.4",
    "lint-staged": ">=10",
    "prettier": "^2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3"
  },
  "private": true,
  "lint-staged": {
    "*.{json,js,jsx,ts,tsx,css,scss,md}": "prettier --config .prettierrc --write",
    "*.{ts,tsx,js,jsx,json}": "eslint --cache --max-warnings 0"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|@react-native-community|expo|@expo|@tanstack/react-query)/)"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
